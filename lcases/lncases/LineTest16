describeString Identifier
( LPAREN
maybeString Identifier
: COLON
String Identifier
? QUEST_NO_WS
) RPAREN
: COLON
String Identifier
{ LCURL
if IF
( LPAREN
maybeString Identifier
!= EXCL_EQ
null NullLiteral
&& CONJ
maybeString Identifier
. DOT
length Identifier
> RANGLE
0 IntegerLiteral
) RPAREN
{ LCURL
